<UserControl x:Class="Lama.UI.UC.TournoiControls.StatistiquesControls.StatistiquesModelesUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Lama.UI.UC.TournoiControls.StatistiquesControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

    <StackPanel>
        <TextBlock Margin="8" FontSize="10" Foreground="{StaticResource IdealForegroundDisabledBrush}" Text="Élimination(s) / Mort(s) / Assistance(s) / Batiment(s) détruit(s)." />
        <WrapPanel>
            <Label Foreground="{StaticResource AccentBaseColorBrush}" Content="Gagnant" />
            <Label Foreground="Red" Content="Perdant" />
        </WrapPanel>
    </StackPanel>

</UserControl>
